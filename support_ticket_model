class SupportTicket {
  String id;
  String userId;
  String subject;
  String message;
  String status;
  DateTime createdAt;
  DateTime updatedAt;

  SupportTicket({
    required this.id,
    required this.userId,
    required this.subject,
    required this.message,
    required this.status,
    required this.createdAt,
    required this.updatedAt,
  });
}
